package main

import "github.com/01-edu/z01"

func main() {
	var A rune
	A = 'a'
	A = 0 
	if A < 1 {
		z01.PrintRune('a')
		A++
	}
	z01.PrintRune('\n')
}

package main

import "github.com/01-edu/z01"

func main() {
	var A rune
	A = 'a'
	A = 0 
	if A < 1 {
		z01.PrintRune('a')
		A++
	}
	z01.PrintRune('b')
	z01.PrintRune('c')
	z01.PrintRune('d')
	z01.PrintRune('e')
	z01.PrintRune('f')
	z01.PrintRune('\n')
}
